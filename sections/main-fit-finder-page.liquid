      <div class="page-header-title title h1 text-center">
        <h1>{{ section.settings.page_header}}</h1>
      </div>
    {% else %}
      <div class="page-header-title title h1 text-center">
        <h1>{{ page.title }}</h1>
      </div>
    {% endif %}
  </div>

