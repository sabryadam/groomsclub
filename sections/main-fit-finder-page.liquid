        {%- if section.settings.continue_btn_link != blank -%}
          <a type="button" href="{{ section.settings.continue_btn_link }}" class="button link hidden continue-shopping-btn continue_shopping_fit_finder" style="display: none;">{{ section.settings.continue_btn_title }} <i class="fas fa-arrow-right"></i></a>
        {%- endif -%}
      </div>
    </div>
  </div>
</div>

<script>

$(document).ready(function(){
  $(".continue_shopping_fit_finder").click(function(e){
    e.preventDefault();
    var collection_url = $(this).attr("href");
    var bring_back_url = localStorage.getItem("bring_back_fit_finder_url");
    if(bring_back_url){
      window.location.href = bring_back_url;
      localStorage.removeItem("bring_back_fit_finder_url");
    }else{
      window.location.href = collection_url;
    }
    
  });
});

</script>

