<div class="page-width-big">
  <div class="page-top-headpadding-flex">
      <div class="breadcrumb">
          <div class="breadcrumb-links">
              <a href="/collections/all">Shop Collection</a>
              <span class="breadcrumb__sep">{% include 'breadcrumb-arrow' %}</span>
              <span class="active-page">{{ product.title | escape }}</span>
          </div>
      </div>
  </div> 
</div>

<section class="page-width normal-product">
  <div class="singal-product product grid grid--1-col {% if product.media.size > 0 %}grid--2-col-tablet{% else %}product--no-media{% endif %}">
    <div class="grid__item product__media-wrapper">
      <!-- Original Product -->
      <slider-component class="slider-mobile-gutter">
        <a class="skip-to-content-link button visually-hidden" href="#ProductInfo-{{ section.id }}">
          {{ "accessibility.skip_to_product_info" | t }}
        </a>
        <ul class="product-main-image product__media-list list-unstyled slider slider--mobile" role="list">
          {%- for media in product.media -%}
              <li class="product__media-item slider__slide{% if media.media_type != 'image' %} product__media-item--full{% endif %}{% if media.media_type == 'video' or media.media_type == 'external_video' %} product-video{% endif %}" data-media-id="{{ section.id }}-{{ media.id }}" data-image-id="{{ media.id }}">
                {% render 'product-media', media: media, position: forloop.index, loop: section.settings.enable_video_looping, modal_id: section.id %}
              </li>
          {%- endfor -%}
        </ul>
        {%- if product.media.size > 1 -%}
        <div class="product-thumb-image" role="list">
          {%- for media in product.media -%}
            {%- if media.media_type == 'video' or media.media_type == 'external_video' -%}
			        {%- unless media.preview_image.alt == 'hide-image' -%}
              <div class="thumb-image product-video-thumb-img">
                <img srcset="{% if media.preview_image.width >= 288 %}{{ media.preview_image | img_url: '288x' }} 288w,{% endif %}
                          {% if media.preview_image.width >= 576 %}{{ media.preview_image | img_url: '576x' }} 576w,{% endif %}
                          {% if media.preview_image.width >= 550 %}{{ media.preview_image | img_url: '550x' }} 550w,{% endif %}
                          {% if media.preview_image.width >= 1100 %}{{ media.preview_image | img_url: '1100x' }} 1100w{% endif %}"
                          src="{{ media | img_url: '150x150' }}"
                          loading="lazy"
                          width="auto"
                          height="auto"
                          alt="{{ media.preview_image.alt | escape }}" />
              </div>
              {%- endunless -%}
            {%- else -%}
			        {%- unless media.preview_image.alt == 'hide-image' -%}
              <div class="thumb-image">
                <img srcset="{% if media.preview_image.width >= 288 %}{{ media.preview_image | img_url: '288x' }} 288w,{% endif %}
                          {% if media.preview_image.width >= 576 %}{{ media.preview_image | img_url: '576x' }} 576w,{% endif %}
                          {% if media.preview_image.width >= 550 %}{{ media.preview_image | img_url: '550x' }} 550w,{% endif %}
                          {% if media.preview_image.width >= 1100 %}{{ media.preview_image | img_url: '1100x' }} 1100w{% endif %}"
                          src="{{ media | img_url: '150x150' }}"
                          loading="lazy"
                          alt="{{ media.preview_image.alt | escape }}" />
              </div>
			        {%- endunless -%}
            {%- endif -%}
          {% endfor %}
        </div>
        {%- endif -%}
      </slider-component>
      {%- if first_3d_model -%}
        <button
          class="button button--full-width product__xr-button"
          type="button"
          aria-label="{{ 'products.product.xr_button_label' | t }}"
          data-shopify-xr
          data-shopify-model3d-id="{{ first_3d_model.id }}"
          data-shopify-title="{{ product.title | escape }}"
          data-shopify-xr-hidden
        >
          {% render 'icon-3d-model' %}
          {{ 'products.product.xr_button' | t }}
        </button>
      {%- endif -%}
    </div>
    <div class="product__info-wrapper grid__item">
      <div id="ProductInfo-{{ section.id }}" class="product__info-container product__info-container--sticky when-customer-logged">
        {%- assign product_form_id = 'product-form-' | append: section.id -%}
        <select class="single-option-selector hidden">
          {% for variant in product.variants %}
            <option data-variant-image-id="{{ variant.featured_media.id }}" data-var-title="{{ variant.title  }}" value="{{ variant.id }}" data-v-price="{{ variant.price }}" data-img="{% if variant.image == blank %}prod-img{% else %}var-img{% endif %}" data-v-image="{% if variant.image == blank %}{{ productHandle.featured_media.image.src | img_url: 'master' }}{% else %}{{ variant.image.src | img_url : 'master' }}{%endif%}" data-v-id="{{ variant.id }}" data-title="{{ variant.title }}" {% unless variant.available %}disable{% endunless %} {% if current_var == variant.id %}selected{% endif %}>
              {{ variant.title }}
            </option>
          {%- endfor -%}
        </select>
        {%- for block in section.blocks -%}
          {%- case block.type -%}
          {%- when '@app' -%}
            {% render block %}
          {%- when 'text' -%}
            <div class="product__text{% if block.settings.text_style == 'uppercase' %} caption-with-letter-spacing{% elsif block.settings.text_style == 'subtitle' %} subtitle{% endif %}">
              {{- block.settings.text -}}
            </div>
          {%- when 'title' -%}
            <h1 class="product__title h2">
              {{ product.title | escape }}
            </h1>
          {%- when 'price' -%}
            <div class="no-js-hidden pdp-price-part" id="price-{{ section.id }}">
              {%- render 'price', product: product, use_variant: true, show_badges: true, price_class: 'price--large' -%}
            </div>
            {%- comment -%}{%- form 'product', product, id: 'product-form-installment', class: 'installment caption-large' -%}
              <input type="hidden" name="id" value="{{ product.selected_or_first_available_variant.id }}">
              {{ form | payment_terms }}
            {%- endform -%}{%- endcomment -%}
          {%- when 'short_description' -%}
            {%- if block.settings.short_desc != blank -%}
              <div class="product__description rte">
                {{ block.settings.short_desc }}
              </div>
            {%- endif -%}
          {%- when 'collapsible_tab' -%}
            <div class="product__accordion accordion">
              <details>
                <summary>
                  <div class="summary__title">
                    {% render 'icon-accordion', icon: block.settings.icon %}
                    <div class="accordion__title">
                      {{ block.settings.heading | default: block.settings.page.title }}
                    </div>
                  </div>
                  {% render 'icon-caret' %}
                </summary>
                <div class="accordion__content rte">
                  {%- if block.settings.decription -%}
                    {{ product.description }}
                  {%- else -%} 
                    {{ block.settings.content }}
                    {{ block.settings.page.content }}
                  {%- endif -%}
                </div>
              </details>
            </div>
          {%- when 'quantity_selector' -%}
            <div class="product-form__input product-form__quantity">
              <label class="form__label" for="Quantity-{{ section.id }}">
                {{ 'products.product.quantity.label' | t }}
              </label>
              <quantity-input class="quantity">
                <button class="quantity__button no-js-hidden" name="minus" type="button">
                  <span class="visually-hidden">{{ 'products.product.quantity.decrease' | t: product: product.title | escape }}</span>
                  {% render 'icon-minus' %}
                </button>
                <input class="quantity__input"
                    type="number"
                    name="quantity"
                    id="Quantity-{{ section.id }}"
                    min="1"
                    value="1"
                    form="product-form-{{ section.id }}"
                  >
                <button class="quantity__button no-js-hidden" name="plus" type="button">
                  <span class="visually-hidden">{{ 'products.product.quantity.increase' | t: product: product.title | escape }}</span>
                  {% render 'icon-plus' %}
                </button>
              </quantity-input>
            </div>
          {%- when 'popup' -%}
              <modal-opener class="product-popup-modal__opener no-js-hidden" data-modal="#PopupModal-{{ block.id }}">
                <button id="ProductPopup-{{ block.id }}" class="product-popup-modal__button link" type="button" aria-haspopup="dialog">{{ block.settings.link_label | default: block.settings.page.title }}</button>
              </modal-opener>
              <a href="{{ block.settings.page.url }}" class="product-popup-modal__button link no-js">{{ block.settings.link_label }}</a>
          {% when 'productaccordion' %}
                  <div class="question-answer-wrapper question-answer-wrapper-pdp-page">
                        <div class="question-heading">
                          {{ block.settings.title }}
                        </div>
                        <div class="answer-content">
                          {%- if product.metafields.my_fields.product_description_details != blank and block.settings.title == 'Product Details' -%}
                            {{ product.metafields.my_fields.product_description_details }}
                          {%- else -%}
                            {{ block.settings.description }}
                          {%- endif -%}
                        </div>
                  </div>
          {%- when 'share' -%}
            <share-button class="product-share-button">
              <span class="share-button_label">
                Share Now :
              </span>

              {%- render 'share-icons'-%}

              <ul class="product__list-social list-unstyled list-social" role="list" style="display: none;">
                {%- if settings.social_instagram_link != blank -%}
                <li class="list-social__item">
                  <a href="{{ settings.social_instagram_link }}" class="link list-social__link" aria-describedby="a11y-external-message">
                    {% comment %}{%- render 'icon-instagram' -%}
                    <img src="{{ 'footer_instagram.png' | asset_url }}"
                        loading="lazy"
                        class="footer__footer-social-logo"
                    >{% endcomment %}
                    <i class="fab fa-instagram"></i>
                    <span class="visually-hidden">{{ 'general.social.links.instagram' | t }}</span>
                  </a>
                </li>
              {%- endif -%}

                {%- if settings.social_facebook_link != blank -%}
                  <li class="list-social__item">
                    <a href="{{ settings.social_facebook_link }}" class="link list-social__link" aria-describedby="a11y-external-message">
                      {% comment %}{%- render 'icon-facebook' -%} 
                      <img src="{{ 'footer_facebook.png' | asset_url }}"
                              loading="lazy"
                              class="footer__footer-social-logo"
                          >{% endcomment %}
                        <i class="fab fa-facebook-f"></i>
                      <span class="visually-hidden">{{ 'general.social.links.facebook' | t }}</span>
                    </a>
                  </li>
                {%- endif -%}
                {%- if settings.social_pinterest_link != blank -%}
                  <li class="list-social__item">
                    <a href="{{ settings.social_pinterest_link }}" class="link list-social__link" aria-describedby="a11y-external-message">
                      {%- render 'icon-pinterest' -%}
                      <span class="visually-hidden">{{ 'general.social.links.pinterest' | t }}</span>
                    </a>
                  </li>
                {%- endif -%}
                {%- if settings.social_twitter_link != blank -%}
                  <li class="list-social__item">
                    <a href="{{ settings.social_twitter_link }}" class="link list-social__link" aria-describedby="a11y-external-message">
                      {% comment %}{%- render 'icon-twitter' -%}
                          <img src="{{ 'footer_twitter.png' | asset_url }}"
                              loading="lazy"
                              class="footer__footer-social-logo"
                          >{% endcomment %}
                          <i class="fab fa-twitter"></i>
                      <span class="visually-hidden">{{ 'general.social.links.twitter' | t }}</span>
                    </a>
                  </li>
                {%- endif -%}
                {%- if settings.social_tiktok_link != blank -%}
                  <li class="list-social__item">
                    <a href="{{ settings.social_tiktok_link }}" class="link list-social__link" aria-describedby="a11y-external-message">
                      {%- render 'icon-tiktok' -%}
                      <span class="visually-hidden">{{ 'general.social.links.tiktok' | t }}</span>
                    </a>
                  </li>
                {%- endif -%}
                {%- if settings.social_tumblr_link != blank -%}
                  <li class="list-social__item">
                    <a href="{{ settings.social_tumblr_link }}" class="link  list-social__link" aria-describedby="a11y-external-message">
                      {%- render 'icon-tumblr' -%}
                      <span class="visually-hidden">{{ 'general.social.links.tumblr' | t }}</span>
                    </a>
                  </li>
                {%- endif -%}
                {%- if settings.social_snapchat_link != blank -%}
                  <li class="list-social__item">
                    <a href="{{ settings.social_snapchat_link }}" class="link list-social__link" aria-describedby="a11y-external-message">
                      {%- render 'icon-snapchat' -%}
                      <span class="visually-hidden">{{ 'general.social.links.snapchat' | t }}</span>
                    </a>
                  </li>
                {%- endif -%}
                {%- if settings.social_youtube_link != blank -%}
                  <li class="list-social__item">
                    <a href="{{ settings.social_youtube_link }}" class="link list-social__link" aria-describedby="a11y-external-message">
                      {% comment %}{%- render 'icon-youtube' -%}
                      <img src="{{ 'footer_youtube.png' | asset_url }}"
                          loading="lazy"
                          class="footer__footer-social-logo"
                      >{% endcomment %}
                      <i class="fab fa-youtube"></i>
                      <span class="visually-hidden">{{ 'general.social.links.youtube' | t }}</span>
                    </a>
                  </li>
                {%- endif -%}
                {%- if settings.social_vimeo_link != blank -%}
                  <li class="list-social__item">
                    <a href="{{ settings.social_vimeo_link }}" class="link list-social__link" aria-describedby="a11y-external-message">
                      {%- render 'icon-vimeo' -%}
                      <span class="visually-hidden">{{ 'general.social.links.vimeo' | t }}</span>
                    </a>
                  </li>
                {%- endif -%}
                {%- comment -%}<li class="list-social__item--placeholder">{{ 'general.footer.social_placeholder' | t }}</li>{%- endcomment -%}
              </ul>

              {% comment %}<details>
                <summary class="share-button__button button button--tertiary">
                  {% render 'icon-share' %}
                  Share
                  {{ 'general.share.share' | t }}
                </summary>
                <div class="share-button__fallback">
                  <div class="field">
                    <input type="text"
                          class="field__input"
                          id="url"
                          value="{{ shop.url | append: product.url }}"
                          placeholder="{{ 'general.share.share_url' | t }}"
                          onclick="this.select();"
                          readonly
                    >
                    <label class="field__label" for="url">{{ 'general.share.share_url' | t }}</label>
                  </div>
                  <button class="button button--tertiary">
                    {% render 'icon-clipboard' %}
                    {{ 'general.share.copy_to_clipboard' | t }}
                  </button>
                  <span id="ShareMessage-{{ section.id }}" class="share-button__message hidden" role="status" aria-hidden="true">
                    {{ 'general.share.success_message' | t }}
                  </span>
                </div>
              </details>{% endcomment %}
            </share-button>
             
          {%- when 'variant_picker' -%}
            {%- unless product.has_only_default_variant -%}
              {%- if block.settings.picker_type == 'button' -%}
                <variant-radios class="no-js-hidden" data-section="{{ section.id }}" data-url="{{ product.url }}">
                  {%- for option in product.options_with_values -%}
                      <fieldset class="js product-form__input">
                        <legend class="form__label">{{ option.name }}</legend>
                        {%- for value in option.values -%}
                          <div class="swatch-outer-wrapper">
                            <input type="radio" id="{{ section.id }}-{{ option.name }}-{{ forloop.index0 }}"
                                  name="{{ option.name }}"
                                  value="{{ value | escape | downcase }}"
                                  form="product-form-{{ section.id }}"
                                  {% if option.selected_value == value %}checked{% endif %}
                            >
                            <label for="{{ section.id }}-{{ option.name }}-{{ forloop.index0 }}">
                              {{ value }}
                            </label>
                          </div>
                        {%- endfor -%}
                      </fieldset>
                  {%- endfor -%}
                  <script type="application/json">
                    {{ product.variants | json }}
                  </script>
                </variant-radios>
              {%- elsif block.settings.picker_type == 'dropdown' -%}
                <variant-selects class="no-js-hidden" data-section="{{ section.id }}" data-url="{{ product.url }}">
                  {%- for option in product.options_with_values -%}
                    <div class="product-form__input product-form__input--dropdown" data-option="{{ option.name | escape }}">
                      <label class="form__label" for="Option-{{ section.id }}-{{ forloop.index0 }}">
                        {{ option.name }}
                      </label>
                      <div class="select">
                        <select id="Option-{{ section.id }}-{{ forloop.index0 }}"
                          class="select__select"
                          name="options[{{ option.name | escape }}]"
                          form="product-form-{{ section.id }}"
                        >
                          {%- for value in option.values -%}
                            <option value="{{ value | escape }}" {% if option.selected_value == value %}selected="selected"{% endif %}>
                              {{ value }}
                            </option>
                          {%- endfor -%}
                        </select>
                        {% render 'icon-caret' %}
                      </div>
                    </div>


               




                  {%- endfor -%}
               

                  <script type="application/json">
                    {{ product.variants | json }}
                  </script>
                </variant-selects>

                <variant-radios class="no-js-hidden csutomswatch" data-section="{{ section.id }}" data-url="{{ product.url }}">
                  {%- for option in product.options_with_values -%}
                    {% assign is_color = false %}
                    {% assign opt_name = option.name | downcase %}
                    {% if opt_name contains 'color' or opt_name contains 'colour' %}
                      {% assign is_color = true %}
                    {% endif %}
                    {% assign s_value = product.selected_or_first_available_variant.options[forloop.index0] %}
                    {%- if is_color -%}
                      <fieldset class="js product-form__input swatch-product-form text swatch-{{ option.name | downcase }}" data-option-index="{{forloop.index | minus: 1 }}">
                        <legend class="form__label swatch-form-label">{{ option.name }} : </legend>
                        {% assign file_extension = 'png' %}
                        {%- for value in option.values -%}
                          <div class="swatch-outer-wrapper">
                            <span class="tool-tip">{{ value }}</span>
                            <input data-opt_name="{{opt_name}}" type="radio" id="{{ section.id }}-{{ option.name }}-{{ forloop.index0 }}"
                                  name="{{ option.name }}"
                                  value="{{ value  | escape }}"
                                  form="product-form-{{ section.id }}"{% if s_value == value %} checked="checked"{% endif %}
                            >
                              <label for="{{ section.id }}-{{ option.name }}-{{ forloop.index0 }}"
                              style="background-color: {{ value | split: ' ' | last | handle }};
                                background-image: url({{ value | handle | prepend : 'color_' | append: '.' | append: file_extension | file_url }})">
                                <span style="display: none;">{{ value }}</span>
                              </label>
                            </div>
                        {%- endfor -%}
                      </fieldset>
                    {%- else -%}
                      <fieldset class="js product-form__input swatch-product-form text swatch-{{ option.name | downcase }}" data-option-index="{{forloop.index | minus: 1 }}">
                        <legend class="form__label swatch-form-label">{{ option.name }} : </legend>
                        {% assign file_extension = 'png' %}
                        {%- for value in option.values -%}
                          <div class="swatch-outer-wrapper">
                            <input data-opt_name="{{opt_name}}" type="radio" id="{{ section.id }}-{{ option.name }}-{{ forloop.index0 }}"
                                  name="{{ option.name }}"
                                  value="{{ value  | escape }}"
                                  form="product-form-{{ section.id }}"{% if s_value == value %} checked="checked"{% endif %}
                            >
                            <label for="{{ section.id }}-{{ option.name }}-{{ forloop.index0 }}">
                              {{ value }}
                            </label>
                          </div>
                        {%- endfor -%}
                      </fieldset>
                    {%- endif -%}
                  {%- endfor -%}
                  <script type="application/json">
                    {{ product.variants | json }}
                  </script>
                </variant-radios>


                
              {% else %}
                <div class="produ-var-option hidden">
                  <select id="Option-{{ section.id }}-{{ forloop.index0 }}"
                    class="select__select"
                    name="options[{{ option.name | escape }}]"
                    form="product-form-{{ section.id }}"
                  >
                    {% for variant in product.variants %}          			
                      <option value="{{ variant.id | escape }}" data-title="{{ variant.title }}">
                        {{ variant.title }}
                      </option>
                    {%- endfor -%}
                  </select>
                </div>
                  <variant-radios class="no-js-hidden" data-section="{{ section.id }}" data-url="{{ product.url }}">
                    {%- for option in product.options_with_values -%}
                      {% assign is_color = false %}
                      {% assign opt_name = option.name | downcase %}
                      {% if opt_name contains 'color' or opt_name contains 'colour' %}
                        {% assign is_color = true %}
                      {% endif %}
                      {% assign s_value = product.selected_or_first_available_variant.options[forloop.index0] %}
                      {%- if is_color -%}
                        <fieldset class="js product-form__input swatch-product-form text swatch-{{ option.name | downcase }}" data-option-index="{{forloop.index | minus: 1 }}">
                          <legend class="form__label swatch-form-label">{{ option.name }} : </legend>
                          {% assign file_extension = 'png' %}
                          {%- for value in option.values -%}
                            <div class="swatch-outer-wrapper">
                              <span class="tool-tip">{{ value }}</span>
                              <input data-opt_name="{{opt_name}}" type="radio" id="{{ section.id }}-{{ option.name }}-{{ forloop.index0 }}"
                                    name="{{ option.name }}"
                                    value="{{ value  | escape }}"
                                    form="product-form-{{ section.id }}"{% if s_value == value %} checked="checked"{% endif %}
                              >
                                <label for="{{ section.id }}-{{ option.name }}-{{ forloop.index0 }}"
