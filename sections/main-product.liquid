{{ 'product-page.js' |  asset_url | script_tag  }}
{% assign current_var = product.selected_or_first_available_variant.id %}
<input type="hidden" class="product-type" value="{{ product.type | downcase | handleize }}" />
<input type="hidden" class="order-type" value="{{ cart.attributes.order_type }}">
<style>
  @media(min-width : 768px)   {
    .sm-hide {
      display: none !important;
    }
  }
  @media(max-width : 767px)   {
    .xs-hide {
      display: none !important;
    }
    .product-thumb-image{
      display: none;
    }
  }
  .mobile-product-image video,
  .mobile-product-image img{
    width: 100%;
    height: auto;
    max-width: 100%;
  }
  .slick-arrow{
    background: transparent;
    border: none;
    width: 15px !important;
  }
  .slick-next.slick-arrow {    
    right: 0%;
  }
  .product-main-image .product-video video{
    width: 100%;
  }
  .banner-slider_container .sliderinner_container{
    padding: 100px 0;
  }
  @media(max-width: 767px) {
    .banner-slider_container .sliderinner_container{
      padding: 50px 0;
    } 
  }
</style>

<script>
  function newLargeImage(x) {
  let clickedImage = x.dataset.mediaId;
  const newImage = document.querySelector(`[data-media-id="${clickedImage}"]`);
  const parentData = newImage.parentElement;
  parentData.prepend(newImage);
  newImage.style.display = "block";
  };
  </script>
