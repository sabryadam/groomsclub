<body
  class="{{ body_class | strip | strip_newlines | split: '|' | join: ' '}}{% if template == 'index' %} homepage{% endif %}{% unless customer %} customer-has-not-logged{% endunless %} template-{{ product.handle}}">
  <!-- Google Tag Manager (noscript) -->
  <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-WPZ3HG7" height="0" width="0"
      style="display:none;visibility:hidden"></iframe></noscript>
  <!-- End Google Tag Manager (noscript) -->
  <a class="skip-to-content-link button visually-hidden" href="#MainContent">
    {{ "accessibility.skip_to_text" | t }}
  </a>
  {% section 'announcement-bar' %}
  {% section 'header' %}
  <main id="MainContent" class="content-for-layout focus-none" role="main" tabindex="-1">
    <input type="hidden" id="custom_id_num" value="{{ customer.id }}">
    {%- if customer -%}
    <input type="hidden" id="custom_email" value="{{ customer.email }}">
    {%- endif -%}
    <div class="page-loader hidden">
      <div class="loading-overlay__spinner ">
        <svg aria-hidden="true" focusable="false" role="presentation" class="spinner" viewBox="0 0 66 66"
          xmlns="http://www.w3.org/2000/svg" width="25" height="25">
          <circle class="path" fill="none" stroke-width="6" cx="33" cy="33" r="30"></circle>
        </svg>
      </div>
    </div>
    {{ content_for_layout }}
  </main>
  {% section 'footer' %}
  <ul hidden>
    <li id="a11y-refresh-page-message">{{ 'accessibility.refresh_page' | t }}</li>
  </ul>
  <script>
    window.routes = {
      cart_add_url: '{{ routes.cart_add_url }}',
      cart_change_url: '{{ routes.cart_change_url }}',
      cart_update_url: '{{ routes.cart_update_url }}'
    };
    window.cartStrings = {
      error: `{{ 'sections.cart.cart_error' | t }}`,
      quantityError: `{{ 'sections.cart.cart_quantity_error_html' | t }}`
    }
    window.variantStrings = {
      addToCart: `{{ 'products.product.add_to_cart' | t }}`,
      soldOut: `{{ 'products.product.sold_out' | t }}`,
      unavailable: `{{ 'products.product.unavailable' | t }}`,
    }
  </script>
  <script src="{{ 'slider.js' | asset_url }}" defer="defer"></script>
  {%- if template contains 'product' -%}
  <script src="{{ 'variants.js' | asset_url }}" defer="defer"></script>
  {%- endif -%}
  <script src="{{ 'custom.js' | asset_url }}" defer></script>

  
  <script>
    {%- if customer -%}
    if (!localStorage.getItem("customerToken")) {
      var custom_id_num = $("#custom_id_num").val();
      var url = theme_custom.api_base_url + '/api/customer/getToken/' + custom_id_num;
      if ($(".template-index").length > 0) {
        $.get(url, function (data, status) {
          localStorage.setItem("customerToken", data.data.token);
        });
      }
    }
    {%- endif -%}
    if (localStorage.getItem("save-fit-finder-flag") == "true" && $("#custom_email").length > 0) {
      localStorage.setItem("save-fit-finder-flag", "false");
      localStorage.setItem("save-fit-finder-flag-replace", "true");
      window.location.href = '/account';
    }
    if (window.location.href.indexOf("virtual-product") > -1) {
      window.location.href = '/collections/all';
    }
    if ($(".template-page-customize-your-look").length == 0 && $(".template-page-home-try-on-checkout").length == 0) {
      // if (window.location.href.indexOf('-pants') > -1 || window.location.href.indexOf('-jacket') > -1 || window.location.href.indexOf('-vest') > -1) {
       if (window.location.href.indexOf('-pants') > -1 || window.location.href.indexOf('-jacket') > -1) {
         const match = window.location.pathname.match(/(?<=products\/).+?(?=-(jacket|suit|pants))/)[0]
         window.location.href = `/products/${match}-suit`
      }
    }

    $(document).ready(function(){

      $(".cart-page .header-bottom-link a.link").click(function(e){
        e.preventDefault();
        var tar = $(this).attr("href");
        history.back();
      });

      

    });

  </script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/html2canvas/1.4.1/html2canvas.min.js" integrity="sha512-BNaRQnYJYiPSqHHDb58B0yaPfCu+Wgds8Gp/gU33kqBtgNS4tSPHuGibyoeqMV/TJlSKda6FXzoEyYGjTe+vXA==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
</body>

